{
    "marginTop": 25,
    "marginLeft": 12,
    "marginBottom": 12,
    "marginRight": 12,
    "orientation": "Portrait",
    "pageSize": "A4",
    "header": {
        "html": "header.html",
        "spacing": 10
    },
    "footer": {
        "html": "footer.html"
    },
    "parts": {
        "cover": {
            "type": "cover",
            "html": "cover.html"
        },
        "info": {
            "type": "content",
            "html": "info.html"
        },
        "toc": {
            "type": "toc",
            "xslStyleSheet": "toc.xslt"
        },
        "content": {
            "type": "content",
            "html": "content.html",
            "data": {
                "captions": ["table", "figure", "code", "formula"]
            }
        }
    }
}